{
  "presets": [
    [
      "env",
      {
        "node": "current"
      }
    ]
  ],

  "plugins": [
    [
      "transform-runtime",
      {
        "regenerator": true
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@src": "./src",
          "@api": "./src/api"
        }
      }
    ]
  ]
}
