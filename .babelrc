{
  "presets": [
    [
      "env",
      {
        "node": "current"
      }
    ]
  ],

  "plugins": [
    [
      "transform-runtime",
      {
        "regenerator": true
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "#routes": "./src/routes",
          "#config": "./src/config",
          "#app": "./src/app"
        }
      }
    ]
  ]
}
